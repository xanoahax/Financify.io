{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "financify.io",
  "version": "0.9.47",
  "identifier": "io.financify.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build -- --base=./"
  },
  "app": {
    "windows": [
      {
        "title": "financify.io",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/xanoahax/Financify.io/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDFDREZENkVCQkRFODQ1MzEKUldReFJlaTk2OWJmSEpNcFkzRkFwMm5icExEazVVNEsxKytBR1QzOTVzYzZLSzMzdTljd3hiNkkK"
    }
  }
}
